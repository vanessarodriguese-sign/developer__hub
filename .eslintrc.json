{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"next/core-web-vitals",
		"eslint:recommended",
		"plugin:react/recommended",
		"standard-with-typescript",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"project": "./tsconfig.json",
		"sourceType": "module"
	},
	"rules": {},
	"overrides": [
		{
			"files": ["*.ts", "*.tsx"],
			"rules": {
				"@typescript-eslint/explicit-function-return-type": "off"
			}
		}
	]
}

