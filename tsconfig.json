{
	"compilerOptions": {
		"lib": ["dom", "dom.iterable", "esnext"],
		"types": ["cypress", "node"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false,
		"strictNullChecks": true,
		"noEmit": true,
		"incremental": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"@/components/*": ["./components/*"]
		}
	},
	"include": [
		"next-env.d.ts",
		".next/types/**/*.ts",
		"cypress/**/*.ts",
		"**/*.ts",
		"**/*.tsx",
		"next.config.js",
		"tailwind.config.js",
		"components/MainButtontsx"
	],
	"exclude": ["node_modules", "./cypress/*"]
}
